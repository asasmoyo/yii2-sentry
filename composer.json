{
    "name": "asasmoyo/yii2-sentry",
    "description": "Yii 2 plugin to integrate Yii 2 application with sentry",
    "keywords": ["yii2", "sentry"],
    "type": "yii2-extension",
    "require": {
        "yiisoft/yii2": "^2.0",
        "sentry/sentry": "^0.22.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^5.4"
    },
    "license": "MIT Licence",
    "authors": [
        {
            "name": "Arba Sasmoyo",
            "email": "asasmoyo@student.uns.ac.id"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "asasmoyo\\yii2sentry\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "asasmoyo\\yii2sentry\\tests\\": "tests"
        }
    },
    "extra": {
        "bootstrap": "asasmoyo\\yii2sentry\\Bootstrap"
    }
}
